{
  "name": "izeat-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint && yarn check-types",
    "check-types": "tsc --noEmit",
    "update-types-local": "npx supabase gen types typescript --local > src/server/supabase/supabaseTypes.ts",
    "update-types-remote": "npx supabase gen types typescript --project-id \"gxoxnnkbkveoeoybhlwx\" --schema public > src/server/supabase/supabaseTypes.ts",
    "prepare:local": "yarn update-types-local && yarn prisma db pull && yarn prisma generate && yarn prisma-case-format --file prisma/schema.prisma  && yarn prisma generate",
    "prepare:remote": "yarn update-types-remote && yarn prisma db pull && yarn prisma generate && yarn prisma-case-format --file prisma/schema.prisma  && yarn prisma generate",
    "db:reset": "npx supabase db reset",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:diff": "npx supabase db diff -f",
    "fmt": "prettier --write ."
  },
